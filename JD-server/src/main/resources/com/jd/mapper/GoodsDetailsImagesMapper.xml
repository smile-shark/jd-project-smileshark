<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jd.mapper.GoodsDetailsImagesMapper">
    <sql id="selectByGoodsIdSQL">
        select * from goods_details_images
        <where>
            <if test="goodsId!=null">
                goods_id=#{goodsId}
            </if>
        </where>
    </sql>
    <resultMap id="selectByGoodsIdMap" type="goodsDetailsImages"/>
    <select id="selectByGoodsId" resultMap="selectByGoodsIdMap">
        <include refid="selectByGoodsIdSQL"/>
    </select>
</mapper>