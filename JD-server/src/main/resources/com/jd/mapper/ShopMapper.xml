<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jd.mapper.ShopMapper">
    <select id="selectByShopId" resultType="shop">
        select shop_id,shop_name from shop
        <where>
            <if test="shopId!=null">
                shop_id=#{shopId}
            </if>
        </where>
    </select>
</mapper>
